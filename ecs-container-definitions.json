[
    {
        "name": "nginx-container",
        "image": "nginx:latest",
        "essential": true,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 80
            }
        ],
        "memory": 512,
        "cpu": 256,
        "environment": [
            {
                "name": "BACKEND_URL",
                "value": ""
            }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "/ecs/nginx-container",
                "awslogs-region": "ap-northeast-1",
                "awslogs-stream-prefix": "ecs"
            }
        }
    }
]